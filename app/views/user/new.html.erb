<h1>User#new</h1>

<%= form_with scope: :user ,url: create_user_path,local: true do |form| %>
  <p>
    <%= form.label :username %><br>
    <%= form.text_field :username %>
  </p>

  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>

  <p>
  <%= form.label :Password %><br>
  <%= form.password_field :password %><br>
  </p>

  <p>
    <%= form.submit %>
  </p>

 
  
<% end %>




